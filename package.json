{
  "name": "app-name",
  "version": "0.0.1",
  "description": "App description",
  "private": true,
  "author": "Sarah Gazelle",
  "license": "MIT",
  "engines": {
    "node": "10.15.0"
  },
  "config": {
    "entrypoint": "build/server.js"
  },
  "scripts": {
    "build": "tsc",
    "clean-build": "rm -rf build && npm run build",
    "start": "node $npm_package_config_entrypoint",
    "test": "mocha build/**/*.test.js",
    "build-test": "npm run build && npm run test",
    "watch-ts": "tsc -w",
    "watch-js": "nodemon $npm_package_config_entrypoint",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,magenta.bold\" \"npm run watch-ts\" \"npm run watch-js\""
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-fileupload": "^1.1.6",
    "express-http-context": "^1.2.0",
    "express-validator": "^5.3.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.11",
    "@types/cls-hooked": "^4.2.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^0.4.0",
    "@types/express-validator": "^3.0.0",
    "@types/node": "^11.9.5",
    "@types/response-time": "^2.3.4",
    "@types/rewire": "^2.5.28",
    "@types/sinon": "^9.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "dotenv": "^6.2.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "prettier": "^1.16.4",
    "rewire": "^4.0.1",
    "sinon": "^9.0.2",
    "typescript": "^3.3.3333"
  }
}
